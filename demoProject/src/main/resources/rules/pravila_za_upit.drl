end

rule "118 dokazi"
	no-loop  true
	when
		$d : Delo(clan == 118);
		
		$pd : PodaciODelu(this.compareDelo($d));
		
		$dk : Dokaz(clan == 0);
	then
		$pd.addToList($dk);
		System.out.println($pd);
		update($pd);
		
		
end

rule "310 dokazi"
	no-loop true
	when 
		$d : Delo(clan == 310);
		
		$pd : PodaciODelu(this.compareDelo($d));
		
		$dk : Dokaz(clan == 0 || clan == 310);
	then
		$pd.addToList($dk);
		System.out.println($pd);
		update($pd);
end

